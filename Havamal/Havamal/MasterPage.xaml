<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Havamal.MasterPage"
             xmlns:local="clr-namespace:Havamal.Views"
             xmlns:pages="clr-namespace:Havamal">
  <MasterDetailPage.Master>
    <pages:MasterPageMaster x:Name="masterPageMaster" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <local:VersePage/>
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>